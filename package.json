{
  "name": "@valkyrie-keymaster/app",
  "private": true,
  "description": "Password and encryption management tool",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build": "turbo typecheck build",
    "dev": "turbo dev",
    "start": "turbo start",
    "test": "turbo test",
    "test-dev": "vitest --passWithNoTests --coverage --coverage.all=false",
    "test-ui": "vitest --ui --passWithNoTests --coverage --coverage.all=false",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,md}\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "1.6.0",
    "husky": "^9.0.11",
    "turbo": "^2.0.4",
    "vitest": "1.6.0"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
